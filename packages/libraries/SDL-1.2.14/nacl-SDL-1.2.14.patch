+static int gNaclVideoWidth;
+static int gNaclVideoHeight;
+
+static int kNaClFlushDelayMs = 20;
+
+#include "SDL_nacl.h"
+
+extern "C" {
+
+#include "SDL_video.h"
+#include "SDL_mouse.h"
