diff -Naur CGAL-3.6/src/CGAL/Makefile CGAL-3.6.nacl/src/CGAL/Makefile
--- CGAL-3.6/src/CGAL/Makefile	1969-12-31 17:00:00.000000000 -0700
+++ CGAL-3.6.nacl/src/CGAL/Makefile	2010-05-20 13:51:36.000000000 -0600
@@ -0,0 +1,63 @@
+CGAL_ROOT = ../..
+
+CXXDEFINES = -DCGAL_CFG_NO_NEXTAFTER \
+             -DCGAL_CFG_NO_STATEMENT_EXPRESSIONS
+
+INCLUDE = -I$(CGAL_ROOT)/include
+
+OBJ_FILES = \
+   assertions.o \
+   Bbox_2_intersections.o \
+   Bbox_3_intersections.o \
+   Color.o \
+   File_header_extended_OFF.o \
+   File_header_OFF.o \
+   File_scanner_OFF.o \
+   File_writer_inventor.o \
+   File_writer_OFF.o \
+   File_writer_VRML_2.o \
+   File_writer_wavefront.o \
+   Interval_arithmetic.o \
+   io.o \
+   JAMA_numeric_solver.o \
+   KDS_Log.o \
+   kernel.o \
+   MP_Float.o \
+   NefPolynomial.o \
+   numeric_solvers_support.o \
+   primes.o \
+   Random.o \
+   Real_timer.o \
+   Residue_type.o \
+   test_FPU_rounding_mode.o \
+   Timer.o \
+   Turkowski_numeric_solver.o \
+
+LIB_CGAL = libCGAL.a
+
+OBJ_DIR = obj
+
+all: $(LIB_CGAL)
+	@echo "Making libCGAL"
+
+install:
+	@echo "Installing CGAL"
+	$(shell cd $(PKG_CONFIG_LIBDIR); \
+			rm -rf $(LIB_CGAL))
+	$(shell cd $(NACL_INCLUDE); \
+			rm -rf CGAL)
+	cp $(LIB_CGAL) $(PKG_CONFIG_LIBDIR)
+	$(shell cd $(PACKAGE_PATH)/include; \
+			tar cf - --exclude='Geomview_stream.h' CGAL | ( cd $(NACL_INCLUDE); tar xfp -))
+
+clean:
+	rm -rf $(OBJ_DIR) $(LIB_CGAL)
+
+$(OBJ_DIR):
+	mkdir $(OBJ_DIR)
+
+$(OBJ_FILES): %.o: %.cpp $(OBJ_DIR)
+	$(CXX) -c $(INCLUDE) $(CXXFLAGS) $< -o $(OBJ_DIR)/$@
+
+$(LIB_CGAL): $(OBJ_DIR) $(OBJ_FILES)
+	cd $(OBJ_DIR) && $(AR) rcs ../$(LIB_CGAL) $(OBJ_FILES)
